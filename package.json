{
  "name": "webamp-ipfs",
  "version": "1.0.0",
  "description": "Webamp + ipfs",
  "scripts": {
    "copy-public": "mkdir -p dist && cp -r public/* dist && echo public copied",
    "start": "npm run copy-public && parcel src/index.html",
    "build": "rm -rf dist; npm run copy-public && parcel build --no-source-maps src/index.html"
  },
  "keywords": ["webamp", "ipfs", "music", "player"],
  "author": "Alberto Miranda",
  "license": "ISC",
  "dependencies": {
    "ipfs": "^0.59.1",
    "ipfs-core": "^0.11.1",
    "mime-types": "^2.1.33",
    "multiformats": "^9.4.10",
    "webamp": "^1.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "parcel": "^2.0.0"
  },
  "browserslist": ["last 2 Chrome versions"]
}
